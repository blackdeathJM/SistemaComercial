            "@s-layout/*": [
                "apps/sistema-comercial/src/app/layout/*",
                "apps/sistema-comercial/src/app/layout/common/*",
                "apps/sistema-comercial/src/app/layout/layouts/*",
                "apps/sistema-comercial/src/app/layout/layouts/empty/*",
                "apps/sistema-comercial/src/app/layout/layouts/horizontal/*",
                "apps/sistema-comercial/src/app/layout/layouts/vertical/*"
            ],
            "@s-admin/*": [
                "apps/sistema-comercial/src/app/modules/admin/*",
                "apps/sistema-comercial/src/app/modules/admin/deptos/*",
                "apps/sistema-comercial/src/app/modules/admin/empleado/*"
