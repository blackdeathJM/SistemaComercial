                "apps/sistema-comercial/src/app/modules/dir-admon-finanzas/*",
                "apps/sistema-comercial/src/app/modules/dir-admon-finanzas/almacen/*",
                "apps/sistema-comercial/src/app/modules/dir-admon-finanzas/compras-servicios/*",
                "apps/sistema-comercial/src/app/modules/dir-admon-finanzas/contabilidad/*",
                "apps/sistema-comercial/src/app/modules/dir-admon-finanzas/egresos/*",
                "apps/sistema-comercial/src/app/modules/dir-admon-finanzas/recursos-humanos/*"
            ],
            "@s-dir-tecnica-operativa/*": [
                "apps/sistema-comercial/src/app/modules/dir-tecnica-operativa/*",
                "apps/sistema-comercial/src/app/modules/dir-tecnica-operativa/telemetria/*"
            ],
            "@s-error/*": [
                "apps/sistema-comercial/src/app/modules/error/*"
            ],
            "@s-general/*": [
                "apps/sistema-comercial/src/app/modules/general/*",
                "apps/sistema-comercial/src/app/modules/general/mis-documentos/*",
                "apps/sistema-comercial/src/app/modules/general/mis-resguardos/*",
                "apps/sistema-comercial/src/app/modules/general/ordenes-atencion/*"
            ],
            "@s-Inicio/*": [
                "apps/sistema-comercial/src/app/modules/inicio/*"
            ],
            "@s-presidencia/*": [
                "apps/sistema-comercial/src/app/modules/presidencia/*"
            ],
            "@s-assets/*": [
                "apps/sistema-comercial/src/assets/fonts/*",
                "apps/sistema-comercial/src/assets/i18n/*",
                "apps/sistema-comercial/src/assets/icons/*",
                "apps/sistema-comercial/src/assets/images/*",
                "apps/sistema-comercial/src/assets/styles/*"
            ],
            "@s-directives/*": [
                "apps/sistema-comercial/src/directives/*"
            ],
            "@s-environments/*": [
                "apps/sistema-comercial/src/environments/*"
            ]
        },
        "types": []
    },
    "files": [
        "src/main.ts",
        "src/polyfills.ts"
    ],
    "include": [
        "**/*.d.ts"
    ]
}
