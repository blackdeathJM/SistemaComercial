{
    "projectType": "library",
    "sourceRoot": "libs/services/src",
    "prefix": "sistema-comercial",
    "targets": {
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/services"],
            "options": {
                "jestConfig": "libs/services/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "libs/services/**/*.ts",
                    "libs/services/**/*.html"
                ]
            }
        }
    },
    "tags": []
}
